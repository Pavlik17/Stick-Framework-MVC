{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["airbnb/base"],
  "rules": {
    "strict": "off",
    "semi": "off",
    "no-plusplus": "off",
    "eol-last": "off",
    "max-len": "off",
    "camelcase": "off",
    "no-tabs": "off",
    "no-alert": "off",
    "prefer-destructuring": "off",
    "indent": "off",
    "no-undef": "off",
    "no-unused-vars": "off",
    "class-methods-use-this": "off",
    "no-shadow": "off",
    "no-param-reassign": "off",
    "func-names": "off",
    "no-lone-blocks": "off",
    "no-restricted-globals": "off",
    "eqeqeq": "off"
  }
}